{
  "name": "mnml new tab",
  "description": "New Tab but only with the things I want",
  "version": "0.0.2",
  "chrome_url_overrides": {
  	"newtab": "index.html"
  },
  "icons": {
	  "16": "./favicon.ico",
	  "32": "./favicon.ico",
	  "48": "./favicon.ico",
	  "128": "./favicon.ico"
  },
  "permissions": ["identity", "storage"],
  "oauth2": {
    "scopes":[
    	"https://www.googleapis.com/auth/calendar.events.readonly"
    ]
  },
  "manifest_version": 2
}
